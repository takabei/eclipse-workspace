def main():
    a = A()
    a.print()
    
    b = B(False)
    b.isOn()
class A:
    def __init__(self, str = "Welcome"):
        self.__str = str
    
    def print(self):
        print(self.__str)
class B:
    def __init__(self, on):
        self.__on = not on
    
    def isOn(self):
        return self.__on
main()
        