from os.path import isfile
def main():
    if isfile("liuyi.txt"):
        print("文件已经存在")
    outfile = open("liuyi.txt", "w")
    outfile.write("liuyi\n")
    outfile.write("test\t\n")
    outfile.write("qwer\n")
    outfile.write("liuyi\n")
    outfile.write("刘义\n")
    outfile.close()
main()
