list1 = []
print("foreach：")
for i in range(3):
    list11 = []
    for n in range(4):
        list11.append(n)
    list1.append(list11)
print(list1)

list2 = []
print("数组下标：")
for row in range(3):
    list2.append([])
    for column in range(4):
        list2[row].append(column)
print(list2)