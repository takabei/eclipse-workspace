def printArea(width = 1, height = 2):
    area = width * height
    print("width:", width, "\theight:", height, "\tarea", area)
printArea()
printArea(1,4)
printArea(width = 1, height = 4)
printArea( height = 4, width = 1)
printArea(width = 1)
printArea(height = 1)