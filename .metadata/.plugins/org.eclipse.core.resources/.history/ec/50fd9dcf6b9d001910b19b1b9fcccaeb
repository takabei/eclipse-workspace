class A:
    def __init__(self, i = 0):
        self.i = i
    def m1(self):
        self.i += 1
class B(A):
    def __init__(self, j = 0, i = 1):
        super().__init__(3)
        self.j = j
        self.i = i
    def m1(self):
        self.i += 1
def main():
    b = B()
    b.m1()
    print("b.i = ", b.super().i)
    print("b.j = ", b.j)

main()